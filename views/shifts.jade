extends layout

block content
    h1= title
    table(class='table table-striped table-bordered table-hover')
            thead 
                tr
                    th Day
                    th Time
                    th GID
                    th Cell Block Name
                    each shift in shifts
                        tr
                            td= shift.Day
                            td= shift.Time_
                            td= shift.GID
                            td= shift.CBname
    div
        button.btn.btn-back(type='button', onclick='history.back()') Go Back
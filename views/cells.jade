extends layout

block content
    h1= title
    table(class='table table-striped table-bordered table-hover')
        thead 
            tr
                th CID
                th Cell Status
                th Maximun Capacity
                th Cell Block Name
                each cell in cells
                    tr
                        td= cell.CID
                        td= cell.CellStatus
                        td= cell.MaxCap
                        td= cell.CBname
    div
        button.btn.btn-back(type='button', onclick='history.back()') Go Back